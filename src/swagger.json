{
  "swagger": "2.0",
  "info": {
    "title": "School Management System API",
    "version": "1.0.0",
    "description": "API Documentation for School Management System"
  },
  "host": "localhost:5000",
  "basePath": "/",
  "schemes": ["http"],
  "paths": {
    "/studentapi/getAllStudents": {
      "get": {
        "summary": "Get all students",
        "responses": {
          "200": { "description": "List of students" }
        }
      }
    },
    "/studentapi/registerStudent": {
      "post": {
        "summary": "Register a new student",
        "parameters": [
          {
            "in": "body",
            "name": "student",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "student_name": { "type": "string" },
                "phone": { "type": "string" },
                "address": { "type": "string" },
                "date_of_birth": { "type": "string", "format": "date" },
                "gender": { "type": "string" },
                "age": { "type": "integer" },
                "enrollment_date": { "type": "string", "format": "date" },
                "emailId": { "type": "string" },
                "password": { "type": "string" },
                "class_id": { "type": "integer" }
              }
            }
          }
        ],
        "responses": {
          "201": { "description": "Student registered successfully" }
        }
      }
    },
    "/studentapi/deleteStudentbyId": {
      "delete": {
        "summary": "Delete a student by ID",
        "parameters": [
          {
            "name": "student_id",
            "in": "query",
            "required": true,
            "type": "integer"
          }
        ],
        "responses": {
          "200": { "description": "Student deleted successfully" }
        }
      }
    },
    "/studentapi/updateStudentbyId": {
      "put": {
        "summary": "Update a student by ID",
        "parameters": [
          {
            "name": "student_id",
            "in": "query",
            "required": true,
            "type": "integer"
          },
          {
            "in": "body",
            "name": "student",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "student_name": { "type": "string" },
                "phone": { "type": "string" },
                "address": { "type": "string" },
                "date_of_birth": { "type": "string", "format": "date" },
                "gender": { "type": "string" },
                "age": { "type": "integer" },
                "enrollment_date": { "type": "string", "format": "date" },
                "emailId": { "type": "string" },
                "password": { "type": "string" },
                "class_id": { "type": "integer" }
              }
            }
          }
        ],
        "responses": {
          "200": { "description": "Student updated successfully" }
        }
      }
    },
    "/instructorapi/getInstructor": {
      "get": {
        "summary": "Get All Instructors",
        "responses": {
          "200": { "description": "List of all instructors" }
        }
      }
    },
    "/instructorapi/createInstructor": {
      "post": {
        "summary": "Create a New Instructor",
        "parameters": [
          {
            "in": "body",
            "name": "instructor",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "instructor_name": { "type": "string" },
                "qualification": { "type": "string" },
                "gender": { "type": "string" },
                "experience": { "type": "number" },
                "phone": { "type": "string" },
                "status": { "type": "string" },
                "current_salary": { "type": "string" },
                "email": { "type": "string" },
                "password": { "type": "string" }
              },
              "required": [
                "instructor_name",
                "qualification",
                "gender",
                "experience",
                "phone",
                "status",
                "current_salary",
                "email",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "201": { "description": "Instructor created successfully" }
        }
      }
    },
    "/instructorapi/deleteInstructor": {
      "delete": {
        "summary": "Delete an Instructor",
        "parameters": [
          {
            "name": "instructor_id",
            "in": "query",
            "required": true,
            "type": "integer"
          }
        ],
        "responses": {
          "200": { "description": "Instructor deleted successfully" }
        }
      }
    },
    "/instructorapi/updateInstructor": {
      "put": {
        "summary": "Update an Instructor",
        "parameters": [
          {
            "name": "instructor_id",
            "in": "query",
            "required": true,
            "type": "integer"
          },
          {
            "in": "body",
            "name": "instructor",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "instructor_name": { "type": "string" },
                "qualification": { "type": "string" },
                "gender": { "type": "string" },
                "experience": { "type": "number" },
                "phone": { "type": "string" },
                "status": { "type": "string" },
                "current_salary": { "type": "string" },
                "email": { "type": "string" },
                "password": { "type": "string" }
              }
            }
          }
        ],
        "responses": {
          "200": { "description": "Instructor updated successfully" }
        }
      }
    },
    "/courseapi/getAllCourses": {
      "get": {
        "summary": "Get All Courses",
        "responses": {
          "200": { "description": "List of all courses" }
        }
      }
    },
    "/courseapi/createCourse": {
      "post": {
        "summary": "Create a New Course",
        "parameters": [
          {
            "in": "body",
            "name": "course",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "course_name": { "type": "string" },
                "description": { "type": "string" },
                "class_id": { "type": "integer" }
              },
              "required": ["course_name", "description", "class_id"]
            }
          }
        ],
        "responses": {
          "201": { "description": "Course created successfully" }
        }
      }
    },
    "/courseapi/deleteCourse": {
      "delete": {
        "summary": "Delete a Course",
        "parameters": [
          {
            "name": "course_id",
            "in": "query",
            "required": true,
            "type": "integer"
          }
        ],
        "responses": {
          "200": { "description": "Course deleted successfully" }
        }
      }
    },
    "/courseapi/updateCourse": {
      "put": {
        "summary": "Update a Course",
        "parameters": [
          {
            "name": "course_id",
            "in": "query",
            "required": true,
            "type": "integer"
          },
          {
            "in": "body",
            "name": "course",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "course_name": { "type": "string" },
                "description": { "type": "string" },
                "class_id": { "type": "integer" }
              }
            }
          }
        ],
        "responses": {
          "200": { "description": "Course updated successfully" }
        }
      }
    }
  }
}
